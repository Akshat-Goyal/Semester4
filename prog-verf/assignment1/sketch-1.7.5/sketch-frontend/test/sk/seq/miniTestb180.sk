pragma options "--bnd-inbits 8";
bit[8] test(int x) implements spec{
	if( x >=128){ x = 127; }
	bit[1024] tmp = ??;
	bit[8] out=0;
	for(int i=0; i<8; ++i){
		out[i] = tmp[x*8 + i];
	}
	return out;
}

bit[8] spec(int x){
	if( x >=128){ x = 127; }
	
	bit[8] out=0;
	
	for(int i=0; i<8; ++i){
		out[i] = (x % 2 ==1);
		x = x/2;
	}
	return out;
}
