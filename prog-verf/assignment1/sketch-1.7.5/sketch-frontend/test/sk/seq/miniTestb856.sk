
pragma options "--fe-fpencoding TO_BACKEND -p lowering --debug-show-dag";

include "math.skh";

harness void main(int x, int y){
    float xf = (float) x;
    float yf = (float) y;
    if(y != 0){
        assert sin(arctan(xf / yf)) == xf/(sqrt(xf*xf+yf*yf));
    }
    assert abs(sin(Pi()/2.0) - (0.5 + 0.5)) < 0.0001;    
}